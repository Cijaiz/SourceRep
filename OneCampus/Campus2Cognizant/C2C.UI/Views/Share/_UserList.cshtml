@model IEnumerable<C2C.BusinessEntities.C2CEntities.UserProfile>
@if (Model.Count() > 0 && Model != null)
{
    foreach (var item in Model)
    {
    <div class="friend-info-share">
        <img src="@item.ProfilePhoto" class="friend-pic imagesize"  width="49" height="49"/>
        <p class="fname">
            @item.FirstName
        </p>
        <input type="hidden" value= '@item.Id' class="sharecheck" />
    </div>
    }
}