      <Setting name="DataPublisherUrl" />
      <Setting name="Diagnostics.EventLogs" />
      <Setting name="Diagnostics.Layout" />
      <Setting name="Diagnostics.Level" />
      <Setting name="Diagnostics.ScheduledTransferPeriod" />
      <Setting name="EmailingServiceUrl" />
      <Setting name="EmailTemplateConfigurationPath" />
      <Setting name="FromAddressId" />
      <Setting name="HubRelyingParty" />
