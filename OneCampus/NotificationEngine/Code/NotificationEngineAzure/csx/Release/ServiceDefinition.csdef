      <Setting name="NotificationEngineStorage" />
      <Setting name="NotificationFeederUrl" />
      <Setting name="OrchardRelyingParty" />
      <Setting name="ScheduleTaskXmlUrl" />
      <Setting name="ServiceIdentityPassword" />
      <Setting name="ServiceIdentityUserName" />
      <Setting name="SftpPassword" />
      <Setting name="SftpServer" />
      <Setting name="SftpUserName" />
